+  const handleGradeCountChange = (size: StrandSize, grade: string, rawValue: string) => {
+    const sanitized = rawValue.replace(/[^0-9]/g, '');
+    const nextStrandCounts: GradeCountDraft = {
+      ...draft.strandCounts,
+      [size]: {
+        ...draft.strandCounts[size],
+        [grade]: sanitized,
